  inherited Q_padrao: TADOQuery
    SQL.Strings = (
      'select '
      ''
      'id_empresa,'
      'razao_social,'
      'n_fantasia,'
      'endereco,'
      'numero,'
      'bairro,'
      'cep,'
      'cidade,'
      'uf,'
      'telefone,'
      'cnpj,'
      'email,'
      'logo,'
      'cadastro'
      ''
      'from empresa'
      ''
      'order by id_empresa')
    Left = 16
    Top = 128
