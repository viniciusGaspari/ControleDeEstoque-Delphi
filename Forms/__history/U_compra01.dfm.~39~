  inherited Q_padrao_item: TADOQuery
    Active = True
    Parameters = <
      item
        Name = 'id_compra'
        Attributes = [paSigned]
        DataType = ftInteger
        Precision = 10
        Value = 8
      end>
    SQL.Strings = (
      'SELECT '
      ' id_sequencia,'
      ' id_compra,'
      ' id_produto,'
      ' qtde,'
      ' vl_custo,'
      ' total_item,'
      ' desconto'
      'FROM item_compra'
      'WHERE id_compra = :id_compra')
