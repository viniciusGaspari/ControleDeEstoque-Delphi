      LookupKeyFields = 'id_fornecedor'
      LookupResultField = 'nome'
      KeyFields = 'ID_FORNECEDOR'
      Size = 30
      Lookup = True
    end
    object Q_padraoDESCRICAO: TStringField
      DisplayLabel = 'Forma de Pgto'
