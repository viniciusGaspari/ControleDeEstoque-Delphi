  inherited Q_padrao: TADOQuery
    Active = True
    SQL.Strings = (
      'SELECT '
      '    ID_COMPRA,'
      '    ID_FORNECEDOR,'
      '    ID_FORMA_PGTO,'
      '    USUARIO,'
      '    CADASTRO,'
      '    VALOR'
      'FROM COMPRA'
      'ORDER BY ID_COMPRA;')
    object Q_padraoID_COMPRA: TAutoIncField
      FieldName = 'ID_COMPRA'
      ProviderFlags = [pfInWhere, pfInKey]
      ReadOnly = True
    end
    object Q_padraoNOME: TStringField
