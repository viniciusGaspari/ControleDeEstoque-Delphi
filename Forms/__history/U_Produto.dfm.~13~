  inherited Q_padrao: TADOQuery
    SQL.Strings = (
      'select'
      ''
      '  a.id_produto,'
      '  a.produto_descricao,'
      '  a.vl_custo,'
      '  a.vl_venda,'
      '  a.estoque,'
      '  a.estoque_min,'
      '  a.unidade,'
      '  a.cadastro,'
      '  a.id_fornecedor,'
      ''
      '  b.nome'
      ''
      ' from produto a, fornecedor b'
      ''
      ' where a.id_fornecedor = b.id_fornecedor;')
    object Q_padraoID_PRODUTO: TAutoIncField
      FieldName = 'ID_PRODUTO'
